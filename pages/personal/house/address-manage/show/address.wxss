.search {
    width: 92vw;
    padding: 0 4vw;
    height: 120rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 28rpx;
    border-bottom: 2rpx solid #ddd;
    background-color: #fff;
}

.search input {
    flex: 1;
    /* width:500rpx; */
}

.search button {
    color: #fff;
    border-radius: 30rpx;
    background-color: #2a9f93;
}

.no-address {
    padding: 50rpx 0;
    text-align: center;
    font-size: 30rpx;
}

.search-item {
    padding: 5rpx 10rpx;
    font-size: 26rpx;
}