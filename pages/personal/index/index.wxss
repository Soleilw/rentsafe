/* 技术支持文字 */
.supportText {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 7vh;
  line-height: 80rpx;
  text-align: center;
  font-size: 25rpx;
  color: #aaa;
}

.tips {
  font-size: 24rpx;
  text-align: center;
  color: #993333;
  padding: 10rpx;
}

/*使屏幕变暗 */
.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.8;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}

/*对话框 */
.commodity_attr_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 280rpx;
  left: 0;
  z-index: 2000;
  padding-top: 20rpx;
  display: flex;
  justify-content: center;
  color: #fff;
}

.guan {
  font-size: 24rpx;
}

.fon {
  font-size: 24rpx;
}

.line {
  line-height: 60rpx;
}

.guanyu {
  width: 400rpx;
  height: 400rpx;
  background: #fff;
}

.flex {
  width: 100%;
  display: flex;
  justify-content: center;
}

.center {
  width: 100%;
  text-align: center;
}

.left {
  width: 100%;
  text-align: left;
}

.solid {
  width: 100%;
  height: 1px;
  background-color: #666;
}

.mag {
  margin: 10px 0;
}

.tex {
  margin-left: 50rpx;
}

.guanyu_img {
  width: 400rpx;
  height: 400rpx;
  display: block;
}

.img_auto {
  width: 50rpx;
  height: 50rpx;
  display: block;
  float: right;
}


.modules {
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin-bottom: 8rpx;
  background-color: #fff;
  font-size: 26rpx;
}

.modules .box {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 50%;
  padding: 20rpx;
}

.modules .hr {
  border-left: 2rpx #ccc solid;
}

.modules .box image {
  width: 60rpx;
  height: 60rpx;
  margin: 12rpx 0;
  border-radius: 50%;
}

.menu {
  display: flex;
  flex-wrap: wrap;

  font-size: 28rpx;
  font-weight: bold;
}

.menu .box {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: calc(25% - 2rpx);
  background-color: #fff;
  margin: 5rpx 0;
  padding: 15rpx 0;
}

.menu .hr {
  margin-right: 2rpx;
}

.menu button {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 25vw;
  margin: 20rpx 0;
}

.menu button image {
  width: 50rpx;
  height: 50rpx;
  margin: 12rpx 0;
  padding: 10rpx;
  border-radius: 50%;
}

.menu .box image {
  width: 50rpx;
  height: 50rpx;
  margin: 12rpx 0;
  padding: 10rpx;
  border-radius: 50%;
}

/* 弹框 */
.framesMask{
  position: fixed;
  left:0;
  top:0;
  width:100vw;
  height:100vh;
  background: rgba(0,0,0,.5);
  z-index: 10000;
}
.framesBox {
  position: fixed;
  left: 10vw;
  top: 30%;
  width: 80vw;
  padding:0 0 0 0;
  border-radius: 20rpx;
  overflow: hidden;
  box-shadow: 0 0 40rpx #666;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: #fff;
  font-size: 30rpx;
  z-index: 10001;
}

.framesBox .framesTitle {
  position: relative;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
  font-size: 35rpx;
  background-color: #2a9f93;
}
.framesBox .framesClose{
  position: absolute;
  right:15rpx;
  top:15rpx;
  width:50rpx;
  height:50rpx;
}
.framesBox textarea {
  border: 3rpx solid #2a9f93;
  border-radius: 20rpx;
  width: 80% !important;
  height:80rpx;
  margin:20rpx 0 0 0;
  text-align: center !important;
  line-height: 80rpx;
  color: #666;
}
.framesBox .framesContent{
  height:200rpx;
  width:100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
}

.framesBox .passwordText {
  margin: 20rpx 0 0 0;
  font-size: 80rpx;
  color: #2a9f93;
}
.framesBox .framesTip{
  margin: 30rpx 0 0 0;
  font-size: 30rpx;
  color: #2a9f93;
}

.framesBox .framesInputBox {
  border-radius: 10rpx;
  width: 300rpx;
  height: 60rpx;
  margin: 20rpx 0 0 0;
  display: flex;
  align-items: stretch;
  justify-content: space-between;
}

.framesBox .framesInputBox button {
  border: 3rpx solid #2a9f93;
  padding: 0;
  margin: 0;
  width:60rpx;
  height:60rpx;
  line-height: 50rpx;
  text-align: center;
  color:#2a9f93;
  background-color: #fff;
}

.framesBox .framesInputBox input {
  border: 3rpx solid #2a9f93;
  border-radius: 10rpx;
  width: 150rpx !important;
  height:55rpx;
  text-align: center !important;
  line-height: 60rpx;
  color: #666;
}

.framesBox .framesBtn {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  margin: 30rpx 0 0 0;
  color: #fff;
  border-radius: 0;
  background-color: #2a9f93;
  font-size: 30rpx;
}